<% if $prospectors { -%> 
filebeat.prospectors: 
 
<% $prospectors.each { |p| -%> 
- type: <% $p['type'] %> 
  enabled: true 
  paths: 
<% $p['paths'].each { |path| -%> 
    - <% $path %> 
<% } -%> 
  json.keys_under_root: <% p['json_keys_under_root'] %> 
<% } -%> 
<% } -%> 

output.logstash: 
  hosts: ["<%= $logstash_server %>:<%= $logstash_port %>"]
